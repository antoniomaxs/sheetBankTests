{
  "root": true,
  "extends": ["labnol"],
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "printWidth": 90,
        "singleQuote": true
      }
    ],
    "prefer-destructuring": [
      "error", {"object": true, "array": false}
    ]
  },
  "globals": {
    "OAuth1": true,
    "OAuth2": true,
    "FirebaseApp": true
  }
}
